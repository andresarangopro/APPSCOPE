        android:theme="@style/MyTheme">
        <activity
            android:name=".Login"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ScreenRegisterUC" />
        <activity android:name=".MiddleLR" />
        <activity android:name=".ScreenRegisterE" />
        <activity android:name=".Perfil" />
        <activity
            android:name=".RegistroEmail"
            android:label="@string/title_activity_registro_email"
            android:theme="@style/MyTheme" />
        <activity android:name=".OfertaTrabajo" />
        <activity android:name=".Oferta" />
        <activity android:name=".Settings" />
